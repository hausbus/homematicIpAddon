   sed -i -e 's/DEV_DESCRIPTION {/DEV_DESCRIPTION {8-fach-Rollladen 8-fach-Rollladen /' $file
   sed -i -e 's/DEV_PATHS       {/DEV_PATHS       {8-fach-Rollladen {{50 \/config\/img\/devices\/50\/187_hbw-lc-sw16-in8-dr_thumb.png} {250 \/config\/img\/devices\/250\/187_hbw-lc-sw16-in8-dr.png}} /' $file
