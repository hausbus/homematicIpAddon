  # 8-fach-Relais
  if grep -q 8-fach-Relais "$file"; then
     echo "$file ist schon fuer Haus-Bus.de 8-fach-Relais konfiguriert"
  else
   if [ ! -f "$file.backup" ];
   then
    echo "Erstelle Backup: $file.backup";
    cp $file $file.backup
   else
     echo "Backup existiert bereits: $file.backup";
   fi
   sed -i -e 's/DEV_LIST {/DEV_LIST {8-fach-Relais /' $file
   sed -i -e 's/DEV_DESCRIPTION {/DEV_DESCRIPTION {8-fach-Relais 8-fach-Relais /' $file
   sed -i -e 's/DEV_PATHS       {/DEV_PATHS       {8-fach-Relais {{50 \/config\/img\/devices\/50\/186_hbw-lc-sw8-dr_thumb.png} {250 \/config\/img\/devices\/250\/186_hbw-lc-sw8-dr.png}} /' $file
   sed -i -e 's/DEV_HIGHLIGHT   {/DEV_HIGHLIGHT   {8-fach-Relais {} /' $file
   echo "$file erfolgreich fuer Haus-Bus.de 8-fach-Relais angepasst";
  fi
  
  # 8-fach-Motormodul
  if grep -q 8-fach-Motormodul "$file"; then
     echo "$file ist schon fuer Haus-Bus.de 8-fach-Motormodul konfiguriert"
  else
   if [ ! -f "$file.backup" ];
   then
    echo "Erstelle Backup: $file.backup";
    cp $file $file.backup
   else
     echo "Backup existiert bereits: $file.backup";
   fi
   sed -i -e 's/DEV_LIST {/DEV_LIST {8-fach-Motormodul /' $file
   sed -i -e 's/DEV_DESCRIPTION {/DEV_DESCRIPTION {8-fach-Motormodul 8-fach-Motormodul /' $file
   sed -i -e 's/DEV_PATHS       {/DEV_PATHS       {8-fach-Motormodul {{50 \/config\/img\/devices\/50\/186_hbw-lc-sw8-dr_thumb.png} {250 \/config\/img\/devices\/250\/186_hbw-lc-sw8-dr.png}} /' $file
   sed -i -e 's/DEV_HIGHLIGHT   {/DEV_HIGHLIGHT   {8-fach-Motormodul {} /' $file
