}

case "$1" in
start)
start
;;
stop)
stop
;;
uninstall)
uninstall
;;
restart)
stop
start
;;

info)
  echo "Info: <b>Haus-Bus.de Smarthome Komponenten</b><br>"
	echo "Info: <a href='http://www.haus-bus.de' target='_blank'>www.haus-bus.de</a>"
    echo "Version: $(cat /usr/local/etc/config/addons/hausbusdeip/VERSION)"
    echo "Name: Haus-Bus.de Smarthome Komponenten"
    echo "Operations: uninstall"
    echo "Update: /addons/hausbusdeip/update-check.cgi"
    ;;
	
*)
start
echo "Usage: $0 {start|stop|restart|uninstall}"
esac
