   sed -i -e 's/DEV_PATHS       {/DEV_PATHS       {4-fach-Taster {{50 \/config\/img\/devices\/50\/177_hbw-sd6-multikey_thumb.png} {250 \/config\/img\/devices\/250\/177_hbw-sd6-multikey.png}} /' $file
   sed -i -e "s/DEV_HIGHLIGHT   {/DEV_HIGHLIGHT   {4-fach-Taster {{\"1\" 0.045 0.140 0.025} {\"2\" 0.85 0.140 0.025} {\"3\" 0.045 0.476 0.025} {\"4\" 1 0.85 0.476 0.025} {\"5\" 1 0.045 0.809 0.025} {\"6\" 1 0.85 0.811 0.025} {\"7\" 1 0.35 0.138 0.02} {\"8\" 1 0.55 0.138 0.02} {\"9\" 1 0.35 0.474 0.02} {\"10\" 1 0.55 0.474 0.02} {\"11\" 1 0.35 0.809 0.02} {\"12\" 1 0.55 0.809 0.02} {\"13\" 3 0.23 0.37 'BL1' 0.2 'verdana' Font.BOLD} {\"14\" 3 0.24 0.37 'IN1' 0.2 'verdana' Font.BOLD} {\"15\" 3 0.24 0.37 'IN2' 0.2 'verdana' Font.BOLD} {\"16\" 3 0.24 0.37 'IN3' 0.2 'verdana' Font.BOLD} {\"17\" 3 0.24 0.37 'IN4' 0.2 'verdana' Font.BOLD} {\"18\" 3 0.17 0.37 'OUT1' 0.2 'verdana' Font.BOLD} {\"19\" 3 0.17 0.37 'OUT2' 0.2 'verdana' Font.BOLD} {\"20\" 3 0.17 0.37 'OUT3' 0.2 'verdana' Font.BOLD} {\"21\" 3 0.17 0.37 'OUT4' 0.2 'verdana' Font.BOLD} {\"22\" 3 0.32 0.37 '%' 0.2 'verdana' Font.BOLD} {\"23\" 3 0.23 0.37 'LUX' 0.2 'verdana' Font.BOLD} {\"24\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"25\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"26\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"27\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"28\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"29\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD}} /" $file
   echo "$file erfolgreich fuer 4-fach-Taster angepasst";
  fi
  
  # 6-fach-Taster-Gira
  if grep -q 6-fach-Taster-Gira "$file"; then
     echo "$file ist schon fuer 6-fach-Taster-Gira konfiguriert"
  else
   if [ ! -f "$file.backup" ];
   then
    echo "Erstelle Backup: $file.backup";
    cp $file $file.backup
   else
     echo "Backup existiert bereits: $file.backup";
   fi
   sed -i -e 's/DEV_LIST {/DEV_LIST {6-fach-Taster-Gira /' $file
   sed -i -e 's/DEV_DESCRIPTION {/DEV_DESCRIPTION {6-fach-Taster-Gira 6-fach-Taster-Gira /' $file
