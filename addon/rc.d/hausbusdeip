  fi
  
  if grep -q 8-fach-Dimmer "$file"; then
     echo "$file ist schon fuer Haus-Bus.de 8-fach-Dimmer konfiguriert"
  else
   if [ ! -f "$file.backup" ];
   then
    echo "Erstelle Backup: $file.backup";
    cp $file $file.backup
   else
     echo "Backup existiert bereits: $file.backup";
   fi
   sed -i -e 's/DEV_LIST {/DEV_LIST {8-fach-Dimmer /' $file
   sed -i -e 's/DEV_DESCRIPTION {/DEV_DESCRIPTION {8-fach-Dimmer 8-fach-Dimmer /' $file
   sed -i -e 's/DEV_PATHS       {/DEV_PATHS       {8-fach-Dimmer {{50 \/config\/img\/devices\/50\/184_hbw-lc-dim8-in8-dr_thumb.png} {250 \/config\/img\/devices\/250\/184_hbw-lc-dim8-in8-dr.png}} /' $file
   sed -i -e 's/DEV_HIGHLIGHT   {/DEV_HIGHLIGHT   {8-fach-Dimmer {} /' $file
   echo "$file erfolgreich fuer Haus-Bus.de 8-fach-Dimmer angepasst";
  fi
  
