  # 2-fach-RGB-Dimmer
  if grep -q 2-fach-RGB-Dimmer "$file"; then
     echo "$file ist schon fuer Haus-Bus.de 2-fach-RGB-Dimmer konfiguriert"
  else
   if [ ! -f "$file.backup" ];
   then
    echo "Erstelle Backup: $file.backup";
    cp $file $file.backup
   else
     echo "Backup existiert bereits: $file.backup";
   fi
   sed -i -e 's/DEV_LIST {/DEV_LIST {2-fach-RGB-Dimmer /' $file
   sed -i -e 's/DEV_DESCRIPTION {/DEV_DESCRIPTION {2-fach-RGB-Dimmer 2-fach-RGB-Dimmer /' $file
   sed -i -e 's/DEV_PATHS       {/DEV_PATHS       {2-fach-RGB-Dimmer {{50 \/config\/img\/devices\/50\/190_hbw-lc-rgbww-in6-dr_thumb.png} {250 \/config\/img\/devices\/250\/190_hbw-lc-rgbww-in6-dr.png}} /' $file
   sed -i -e 's/DEV_HIGHLIGHT   {/DEV_HIGHLIGHT   {2-fach-RGB-Dimmer {} /' $file
   echo "$file erfolgreich fuer Haus-Bus.de 2-fach-RGB-Dimmer angepasst";
  fi
  
  # 6-fach-PWM-Modul
  if grep -q 6-fach-PWM-Modul "$file"; then
     echo "$file ist schon fuer Haus-Bus.de 6-fach-PWM-Modul konfiguriert"
  else
   if [ ! -f "$file.backup" ];
   then
    echo "Erstelle Backup: $file.backup";
    cp $file $file.backup
   else
     echo "Backup existiert bereits: $file.backup";
   fi
   sed -i -e 's/DEV_LIST {/DEV_LIST {6-fach-PWM-Modul /' $file
   sed -i -e 's/DEV_DESCRIPTION {/DEV_DESCRIPTION {6-fach-PWM-Modul 6-fach-PWM-Modul /' $file
   sed -i -e 's/DEV_PATHS       {/DEV_PATHS       {6-fach-PWM-Modul {{50 \/config\/img\/devices\/50\/190_hbw-lc-rgbww-in6-dr_thumb.png} {250 \/config\/img\/devices\/250\/190_hbw-lc-rgbww-in6-dr.png}} /' $file
   sed -i -e 's/DEV_HIGHLIGHT   {/DEV_HIGHLIGHT   {6-fach-PWM-Modul {} /' $file
