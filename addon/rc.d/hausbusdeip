#!/bin/sh

NAME=hausbusdeip
PIDFILE=/var/run/$NAME.pid
LOGFILE=/var/log/$NAME.log

ADDON_DIR=/usr/local/etc/config/addons/${NAME}
WWW_DIR=/usr/local/etc/config/addons/www/${NAME}
RCD_DIR=/usr/local/etc/config/rc.d


start() 
{
  echo 'Starting...'
  cd ${ADDON_DIR}
  ./installHausbus
  
  if [ -f "$PIDFILE" ] && kill -0 $(cat $PIDFILE) ; then
  echo 'Service already running' >&2
  return 1
  fi
  echo 'Starting service.' >&2
  cd /usr/local/etc/config/addons/hausbusdeip/
  /opt/java/bin/java -cp HausBusDeInterface.jar de.hausbus.homematic.HomematicBridge > $LOGFILE 2> /dev/null & echo $!> $PIDFILE
  echo 'Service started' >&2
}

stop() 
{
  if [ ! -f "$PIDFILE" ] || ! kill -0 $(cat $PIDFILE); then
  echo 'Service not running' >&2
  return 1
  fi
  echo 'Stopping service.' >&2
  kill -15 $(cat "$PIDFILE") && rm -f "$PIDFILE"
  echo 'Service stopped' >&2
}

uninstall() 
{
  mount -o remount,rw /
  # remove the addon button
  ${ADDON_DIR}/update_hm_addons.tcl -d ${NAME}

  # remove interface
  sed -i /usr/local/etc/config/InterfacesList.xml -e "s/<ipc><name>HausBusDe<\/name><url>xmlrpc:\/\/127.0.0.1:8766<\/url><info>HausBusDe<\/info><\/ipc>//"

  # remove init script
  #rm /etc/init.d/S60HausBusDeInterface
  
  # remove the whole addon related stuff
  rm -rf ${ADDON_DIR}
  rm -rf ${WWW_DIR}
  rm -f ${RCD_DIR}/${NAME}
  
  # GUI wiederherstellen
  cp /www/config/devdescr/DEVDB.tcl.backup /www/config/devdescr/DEVDB.tcl
  rm /www/config/devdescr/DEVDB.tcl.backup
  cp /www/webui/webui.js.backup /www/webui/webui.js
  rm /www/webui/webui.js.backup
  
  mount -o remount,ro /
}

init()
{
  mount -o remount,rw /
  
  #Aktuelle Patches durchfuehren
  /etc/config/addons/hausbusdeip/patch
  > /etc/config/addons/hausbusdeip/patch
  
  # altes Startscript aufräumen
  #rm /etc/init.d/S60HausBusDeInterface
  
  #Interface eintragen
  if [ $(cat /usr/local/etc/config/InterfacesList.xml | grep '<name>HausBusDe</name>' | wc -l ) -eq 0 ]
  then
    echo "Updating InterfaceList.xml"
    sed -i /usr/local/etc/config/InterfacesList.xml -e "s/<\/interfaces>/\t<ipc><name>HausBusDe<\/name><url>xmlrpc:\/\/127.0.0.1:8766<\/url><info>HausBusDe<\/info><\/ipc>\n<\/interfaces>/"
  fi
  
  #Startscript nach init.d
  cp /etc/config/addons/hausbusdeip/S60HausBusDeInterface /etc/init.d/
  chmod +x /etc/init.d/S60HausBusDeInterface
  #rm /etc/init.d/S60HausBusDeInterface
  
  file="/www/config/devdescr/DEVDB.tcl"
  
  # 6-fach-Taster
  if grep -q 6-fach-Taster\" "$file"; then
     echo "$file ist schon fuer 6-fach-Taster konfiguriert"
  else
   if [ ! -f "$file.backup" ];
   then
    echo "Erstelle Backup: $file.backup";
    cp $file $file.backup
   else
     echo "Backup existiert bereits: $file.backup";
   fi
   sed -i -e 's/DEV_LIST {/DEV_LIST {6-fach-Taster /' $file
   sed -i -e 's/DEV_DESCRIPTION {/DEV_DESCRIPTION {6-fach-Taster 6-fach-Taster /' $file
   sed -i -e 's/DEV_PATHS       {/DEV_PATHS       {6-fach-Taster {{50 \/config\/img\/devices\/50\/177_hbw-sd6-multikey_thumb.png} {250 \/config\/img\/devices\/250\/177_hbw-sd6-multikey.png}} /' $file
   sed -i -e "s/DEV_HIGHLIGHT   {/DEV_HIGHLIGHT   {6-fach-Taster {{\"1\" 0.045 0.140 0.025} {\"2\" 0.85 0.140 0.025} {\"3\" 0.045 0.476 0.025} {\"4\" 1 0.85 0.476 0.025} {\"5\" 1 0.045 0.809 0.025} {\"6\" 1 0.85 0.811 0.025} {\"7\" 1 0.35 0.138 0.02} {\"8\" 1 0.55 0.138 0.02} {\"9\" 1 0.35 0.474 0.02} {\"10\" 1 0.55 0.474 0.02} {\"11\" 1 0.35 0.809 0.02} {\"12\" 1 0.55 0.809 0.02} {\"13\" 3 0.23 0.37 'BL1' 0.2 'verdana' Font.BOLD} {\"14\" 3 0.24 0.37 'IN1' 0.2 'verdana' Font.BOLD} {\"15\" 3 0.24 0.37 'IN2' 0.2 'verdana' Font.BOLD} {\"16\" 3 0.24 0.37 'IN3' 0.2 'verdana' Font.BOLD} {\"17\" 3 0.24 0.37 'IN4' 0.2 'verdana' Font.BOLD} {\"18\" 3 0.17 0.37 'OUT1' 0.2 'verdana' Font.BOLD} {\"19\" 3 0.17 0.37 'OUT2' 0.2 'verdana' Font.BOLD} {\"20\" 3 0.17 0.37 'OUT3' 0.2 'verdana' Font.BOLD} {\"21\" 3 0.17 0.37 'OUT4' 0.2 'verdana' Font.BOLD} {\"22\" 3 0.32 0.37 '%' 0.2 'verdana' Font.BOLD} {\"23\" 3 0.23 0.37 'LUX' 0.2 'verdana' Font.BOLD} {\"24\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"25\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"26\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"27\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"28\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"29\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD}} /" $file
   echo "$file erfolgreich fuer 6-fach-Taster angepasst";
  fi
  
  # 2-fach-Taster
  if grep -q 2-fach-Taster\" "$file"; then
     echo "$file ist schon fuer 2-fach-Taster konfiguriert"
  else
   if [ ! -f "$file.backup" ];
   then
    echo "Erstelle Backup: $file.backup";
    cp $file $file.backup
   else
     echo "Backup existiert bereits: $file.backup";
   fi
   sed -i -e 's/DEV_LIST {/DEV_LIST {2-fach-Taster /' $file
   sed -i -e 's/DEV_DESCRIPTION {/DEV_DESCRIPTION {2-fach-Taster 2-fach-Taster /' $file
   sed -i -e 's/DEV_PATHS       {/DEV_PATHS       {2-fach-Taster {{50 \/config\/img\/devices\/50\/177_hbw-sd6-multikey_thumb.png} {250 \/config\/img\/devices\/250\/177_hbw-sd6-multikey.png}} /' $file
   sed -i -e "s/DEV_HIGHLIGHT   {/DEV_HIGHLIGHT   {2-fach-Taster {{\"1\" 0.045 0.140 0.025} {\"2\" 0.85 0.140 0.025} {\"3\" 0.045 0.476 0.025} {\"4\" 1 0.85 0.476 0.025} {\"5\" 1 0.045 0.809 0.025} {\"6\" 1 0.85 0.811 0.025} {\"7\" 1 0.35 0.138 0.02} {\"8\" 1 0.55 0.138 0.02} {\"9\" 1 0.35 0.474 0.02} {\"10\" 1 0.55 0.474 0.02} {\"11\" 1 0.35 0.809 0.02} {\"12\" 1 0.55 0.809 0.02} {\"13\" 3 0.23 0.37 'BL1' 0.2 'verdana' Font.BOLD} {\"14\" 3 0.24 0.37 'IN1' 0.2 'verdana' Font.BOLD} {\"15\" 3 0.24 0.37 'IN2' 0.2 'verdana' Font.BOLD} {\"16\" 3 0.24 0.37 'IN3' 0.2 'verdana' Font.BOLD} {\"17\" 3 0.24 0.37 'IN4' 0.2 'verdana' Font.BOLD} {\"18\" 3 0.17 0.37 'OUT1' 0.2 'verdana' Font.BOLD} {\"19\" 3 0.17 0.37 'OUT2' 0.2 'verdana' Font.BOLD} {\"20\" 3 0.17 0.37 'OUT3' 0.2 'verdana' Font.BOLD} {\"21\" 3 0.17 0.37 'OUT4' 0.2 'verdana' Font.BOLD} {\"22\" 3 0.32 0.37 '%' 0.2 'verdana' Font.BOLD} {\"23\" 3 0.23 0.37 'LUX' 0.2 'verdana' Font.BOLD} {\"24\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"25\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"26\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"27\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"28\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD} {\"29\" 3 0.31 0.37 'Â°C' 0.2 'verdana' Font.BOLD}} /" $file
   echo "$file erfolgreich fuer 2-fach-Taster angepasst";
  fi
  
  # 4-fach-Taster
  if grep -q 4-fach-Taster\" "$file"; then
     echo "$file ist schon fuer 4-fach-Taster konfiguriert"
